    "@service": "./dist/src/service",
    "@controller": "./dist/src/controller",
    "@dto": "./dist/src/dto",
    "@entity": "./dist/src/entity",
    "@repository": "./dist/src/repository",
    "@helper": "./dist/src/app/helper"

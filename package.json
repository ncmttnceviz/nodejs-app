    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.6.0",
    "@sentry/node": "^7.30.0",
